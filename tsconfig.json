{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "CommonJS",
    "target": "ES2019",
    "lib": [
      "ES2020"
    ],
    "declaration": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
  },
  "include": [
    "function", 
    "cdk", 
    "canary",
  ],
  "exclude": [
    "node_modules", 
    "cdk.out",
    "canary"
  ],
}
